{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run in RoboFont",
      "type": "shell",
      "command": "robofont",
      "args": ["-p", "${file}"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Run Current Python File",
      "type": "shell",
      "command": "python3",
      "args": ["${file}"],
    }
  ]
  ]
}
